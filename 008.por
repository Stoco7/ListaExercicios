programa {
  funcao inicio() {
    inteiro a, b, c, soma

    escreva("Escreva o Valor de a : ")
    leia(a)
    escreva("Escreva o Valor de b : ")
    leia(b)
    escreva("Escreva o Valor de c : ")
    leia(c)

    se(a+b < c){
       escreva("A soma de A + B é menor que C.")
    }
    senao
    escreva("A soma de A + B não é menor que C.")
  }
}
