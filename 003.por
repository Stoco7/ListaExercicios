programa {
  funcao inicio() {
    cadeia nome, telefone, email, cidade

      escreva("\n Informe seu nome: ")
      leia(nome)
    
      escreva("\n Informe seu telefone: ")
      leia(telefone)

      escreva("\n Informe seu e-mail: ")
      leia(email)

      escreva("\n Informe sua Cidade: ")
      leia(cidade)

        limpa()

      escreva("Cadastro de Cliente " ,"\n Nome :",nome, "\n telefone: ",telefone,"\n E-mail :",email, "\n Cidade :", cidade)

  }
}
